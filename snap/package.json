{
  "name": "snap-wasm-example",
  "version": "0.1.0",
  "description": "Snap example using WASM for MetaMask Snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/piotr-roslaniec/snap-wasm-example/snap"
  },
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build:clean": "yarn clean && yarn build",
    "build:website": "node ./scripts/build-website.js && cp snap.manifest.json dist/",
    "build": "yarn wasm:hex && mm-snap build --verboseErrors && cp snap.manifest.json dist/",
    "clean": "rimraf dist",
    "serve": "mm-snap serve",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "wasm:hex": "node ./scripts/wasm-hex.js"
  },
  "dependencies": {
    "wasm-bundler": "file:../wasm-bundler",
    "eth-rpc-errors": "^4.0.3"
  },
  "devDependencies": {
    "@metamask/eslint-config": "^8.0.0",
    "@metamask/eslint-config-jest": "^8.0.0",
    "@metamask/eslint-config-nodejs": "^8.0.0",
    "@metamask/snaps-cli": "^0.10.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=12"
  }
}